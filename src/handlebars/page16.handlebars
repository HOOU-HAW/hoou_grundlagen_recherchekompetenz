<section>
    {{#with page.[15]}}
    <h2 class="icon icon-information">{{headline}}</h2>
    {{#each intro}}
    <p>
        {{.}}
    </p>
    {{/each}}
    <h4 class="icon icon-example">
        {{exampleHeadline}}
    </h4>
    {{#each examples}}
    {{#if intro}}
    <p>
        {{& intro}}
    </p>
    {{/if}}
    <ul>
        {{#each result}}
        <li>
            {{.}}
        </li>
        {{/each}}
    </ul>
    {{#if outro}}
        <p>
            {{& outro}}
        </p>
    {{/if}}
    {{/each}}
    {{/with}}
    {{> pageNavigationFull}}
</section>